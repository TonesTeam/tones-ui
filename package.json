{
    "name": "tones",
    "workspaces": [
        "apps/router",
        "apps/backend",
        "apps/frontend",
        "apps/electron"
    ],
    "version": "1.0.0",
    "description": "tones ui",
    "main": "index.js",
    "scripts": {
        "set-env": "",
        "start-dev:fe": "npm run start-frontend -w apps/frontend",
        "start-dev:be": "npm run start -w apps/backend",
        "start-dev:router": "npm run start -w apps/router",
        "start-dev:electron": "npm run start -w apps/electron",
        "start-dev": "cross-env-shell ROUTER_PORT=8080 FE_PORT=8081 BE_PORT=8082 npm run _start-dev",
        "_start-dev": "npm-run-all -p -r start-dev:router start-dev:fe start-dev:be start-dev:electron",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/drewzxcvbnm/tones-ui.git"
    },
    "author": "drewman",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/drewzxcvbnm/tones-ui/issues"
    },
    "homepage": "https://github.com/drewzxcvbnm/tones-ui#readme",
    "devDependencies": {
        "cross-env": "^7.0.3",
        "npm-run-all": "^4.1.5"
    }
}
