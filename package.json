{
    "name": "tones",
    "workspaces": [
        "modules/router",
        "modules/backend",
        "modules/frontend",
        "modules/electron"
    ],
    "version": "1.0.0",
    "description": "tones ui",
    "main": "index.js",
    "scripts": {
        "set-env": "",
        "start-dev:fe": "npm run start-frontend -w modules/frontend",
        "start-dev:be": "npm run start -w modules/backend",
        "start-dev:router": "npm run start -w modules/router",
        "start-dev:electron": "npm run start -w modules/electron",
        "start-dev:cli": "cross-env-shell ROUTER_PORT=8080 FE_PORT=8081 BE_PORT=8082 npm run _start-dev:cli",
        "_start-dev:cli": "npm-run-all -p -r start-dev:router start-dev:fe start-dev:be",
        "start-dev": "cross-env-shell ROUTER_PORT=8080 FE_PORT=8081 BE_PORT=8082 npm run _start-dev",
        "_start-dev": "npm-run-all -p -r start-dev:router start-dev:fe start-dev:be start-dev:electron",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/drewzxcvbnm/tones-ui.git"
    },
    "author": "drewman",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/drewzxcvbnm/tones-ui/issues"
    },
    "homepage": "https://github.com/drewzxcvbnm/tones-ui#readme",
    "devDependencies": {
        "@types/react-redux": "^7.1.24",
        "cross-env": "^7.0.3",
        "electron-devtools-installer": "^3.2.0",
        "npm-run-all": "^4.1.5"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.8.3",
        "@types/lodash": "^4.14.182",
        "react-redux": "^8.0.2",
        "redux": "^4.2.0",
        "redux-devtools-extension": "^2.13.9",
        "redux-thunk": "^2.4.1"
    }
}
